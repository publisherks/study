        "@types/node": "^20.12.10",
        "@vitejs/plugin-vue": "^5.0.4",
        "@vue/eslint-config-typescript": "^13.0.0",
        "@vue/tsconfig": "^0.5.1",
        "eslint": "~8",
        "eslint-plugin-vue": "^9.25.0",
        "npm-run-all2": "^6.1.2",
        "sass": "^1.77.0",
        "typescript": "~5.4",
        "vite": "^5.2.11",
