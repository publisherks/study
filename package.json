        "@types/js-cookie": "^3.0.6",
        "@types/leaflet": "^1.9.8",
        "@types/node": "^18.19.3",
        "@vitejs/plugin-vue": "^5.0.0",
        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/tsconfig": "^0.5.1",
        "eslint": "^8.56.0",
        "eslint-plugin-vue": "^9.19.2",
        "npm-run-all2": "^6.1.1",
        "sass": "^1.69.6",
        "typescript": "^5.3.3",
        "vite": "^5.0.10",
        "vite-plugin-eslint": "^1.8.1",
        "vue-tsc": "^1.8.27"
    }
