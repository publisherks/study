{
    "name": "vue-standard",
    "version": "0.8.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check build-only",
        "build:dev": "run-p type-check build-only:dev",
        "build-only": "vite build",
        "build-only:dev": "vite build --mode staging",
        "preview": "vite preview",
        "preview:dev": "vite preview --mode staging",
        "type-check": "vue-tsc --build --force",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --ignore-pattern \"/src/assets/*\""
    },
    "dependencies": {
