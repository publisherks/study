        "@vueuse/core": "^10.9.0",
        "@vueuse/math": "^10.9.0",
        "@vueuse/router": "^10.9.0",
        "axios": "1.7.0-beta.0",
        "chart.js": "^4.4.2",
        "dayjs": "^1.11.11",
        "gridstack": "^10.1.2",
        "js-cookie": "^3.0.5",
        "konva": "^9.3.6",
        "leaflet": "^1.9.4",
        "pinia": "2.1.8-beta.0",
        "vee-validate": "^4.12.7",
