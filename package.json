{
    "name": "vue-standard",
    "version": "0.5.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check build-only",
        "build:dev": "run-p type-check build-only:dev",
        "build-only": "vite build",
        "build-only:dev": "vite build --mode staging",
        "preview": "vite preview",
        "preview:dev": "vite preview --mode staging",
        "type-check": "vue-tsc --build --force",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --ignore-pattern \"/src/assets/*\""
    },
    "dependencies": {
        "@sentry/tracing": "^7.91.0",
        "@sentry/vue": "^7.91.0",
        "@vee-validate/yup": "^4.12.4",
        "@vuepic/vue-datepicker": "^7.4.0",
        "@vueuse/core": "^10.7.1",
        "@vueuse/math": "^10.7.1",
        "@vueuse/router": "^10.7.1",
        "axios": "^1.6.3",
        "chart.js": "^4.4.1",
        "dayjs": "^1.11.10",
        "gridstack": "^10.0.1",
        "js-cookie": "^3.0.5",
        "konva": "^9.3.0",
        "leaflet": "^1.9.4",
        "pinia": "^2.1.7",
        "vee-validate": "^4.12.4",
        "vue": "^3.4.0",
        "vue-konva": "^3.0.2",
        "vue-router": "^4.2.5",
        "yup": "^1.3.3"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.6.1",
